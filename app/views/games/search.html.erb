<% @games.each do |game| %>
    <div>
        <form action=<%=new_post_path%> method="GET">
            <p><a href="/games/<%= game.id %>"/><%= game.title %></a>
            <input type="hidden" name="game_id" value= "<%= game.id %>" >
            <input type="submit" value="Create Post"></p>
        </form>
    </div>
    
<% end %>
